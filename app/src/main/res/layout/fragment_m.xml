<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="10dp"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:gravity="left"
        android:text="FACILITY TRANSPORT SYSTEM"
        android:textColor="@color/text_hard"
        android:textSize="16dp"
        android:textStyle="bold" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="8dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:scrollbarSize="50dip">
        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">


            <!-- M1 Starts RB  -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">


                <LinearLayout
                    android:id="@+id/lv_M1"
                    style="@style/Q_Linerlayout_v">


                    <TextView
                        android:id="@+id/L_M1"
                        style="@style/TextView"
                        android:text="@string/L_M1" />


                    <RadioGroup style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/R_M1_yes"
                            style="@style/RadioButton"
                            android:text="@string/R_yes" />

                        <RadioButton
                            android:id="@+id/R_M1_No"
                            style="@style/RadioButton"
                            android:text="@string/R_No" />


                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- M1 End RB -->


            <!-- M2 Starts ED-->
            <android.support.v7.widget.CardView

                style="@style/Q_CardView">


                <LinearLayout

                    android:id="@+id/lv_M2"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        android:id="@+id/L_M2"
                        style="@style/TextView"
                        android:text="@string/L_M2"

                        />


                    <EditText
                        android:id="@+id/ET_M2"
                        style="@style/EditText_number"
                        android:hint="Number"
                        android:maxLength="3" />


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- M2 Ends ED-->


            <!-- M3 Starts ED-->
            <android.support.v7.widget.CardView

                style="@style/Q_CardView">


                <LinearLayout

                    android:id="@+id/lv_M3"
                    style="@style/Q_Linerlayout_v">

