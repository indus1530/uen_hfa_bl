<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.aku.abdulsajid.uen_hfa_bl.Fragment_k">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txt_h"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_marginLeft="10dp"
            android:gravity="left"
            android:text="NEWBORN HEALTH"
            android:textColor="#000"
            android:textSize="15sp"
            android:textStyle="bold"

            />


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:layout_marginBottom="8dp"
            android:fadeScrollbars="false"
            android:fillViewport="true"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="5dp"
            android:scrollbarSize="50dip">
            <!-- Parent layout starts -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <!-- serila number layout starts -->
                <androidx.cardview.widget.CardView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:id="@+id/tableRow4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txt_serial_number"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="10sp"
                            android:text="Mother Name"
                            android:textAppearance="@style/MyTextAppearance" />

                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="10sp"
                            android:text="Mother Name"
                            android:textAppearance="@style/MyTextAppearance" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- serila number  layout ends -->


                <!-- K1 starts  Radio b-->
                <androidx.cardview.widget.CardView style="@style/Q_CardView"

                    >


                    <LinearLayout style="@style/Q_Linerlayout_v">

                        <TextView


                            android:id="@+id/txt_K1"
                            style="@style/TextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/txt_K1"

                            />

                        <RadioGroup style="@style/RadioGroup"

                            >

                            <RadioButton
                                android:id="@+id/rb_K1_yes"
                                style="@style/RadioButton"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/rb_K1_no"
                                style="@style/RadioButton"
                                android:text="@string/rb_no" />

                            <RadioButton
                                android:id="@+id/rb_K1_dnt"
                                style="@style/RadioButton"
                                android:text="@string/rb_dontknow" />


                        </RadioGroup>


                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <!-- K1 Ends  Radio Button-->

                <!-- K2 starts  Radio b-->
